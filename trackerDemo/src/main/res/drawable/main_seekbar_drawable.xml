<?xml version="1.0" encoding="utf-8"?>  

<!-- musicSeekBar的背景 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">  
    <!-- 背景图 -->  
    <item android:id="@android:id/background" android:drawable="@drawable/main_seekbar_background" />  
    <!--全部能量图  -->  
    <item android:id="@android:id/secondaryProgress">  
        <clip android:drawable="@drawable/main_seekbar_background" />  
    </item>  
    <!-- 进和能量图 -->  
    <item android:id="@android:id/progress">  
        <clip android:drawable="@drawable/main_seekbar_progress" />  
    </item>
</layer-list>